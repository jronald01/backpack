{
  "$schema": "https://context7.com/schema/context7.json",
  "projectTitle": "Backpack Design System",
  "description": "Backpack is a collection of design resources, reusable components, and guidelines for creating Skyscanner's products, offering a unified toolkit for web development.",
  "branch": "main",
  "excludeFolders": [
    "__snapshots__",
    ".husky",
    "archive",
    "archived",
    "old",
    "docs/old",
    "deprecated",
    "legacy",
    "previous",
    "outdated",
    "superseded",
    "i18n/zh*",
    "i18n/es*",
    "i18n/fr*",
    "i18n/de*",
    "i18n/ja*",
    "i18n/ko*",
    "i18n/ru*",
    "i18n/pt*",
    "i18n/it*",
    "i18n/ar*",
    "i18n/hi*",
    "i18n/tr*",
    "i18n/nl*",
    "i18n/pl*",
    "i18n/sv*",
    "i18n/vi*",
    "i18n/th*",
    "zh-cn",
    "zh-tw",
    "zh-hk",
    "zh-mo",
    "zh-sg"
  ],
  "excludeFiles": [
    ".editorconfig",
    ".eslintignore",
    ".eslintrc",
    ".gitignore",
    ".gitmodules",
    ".naverc",
    ".npmrc",
    ".nvmrc",
    ".percy.yml",
    ".prettierignore",
    ".prettierrc",
    "stylelintignore",
    ".stylelintrc.json",
    "babel.config.js",
    "dangerfile.ts",
    "LICENSE",
    "license.txt",
    "tsconfig.json",
    "tsconfig.declaration.json",
    "CHANGELOG.md",
    "changelog.md",
    "CHANGELOG.mdx",
    "changelog.mdx",
    "LICENSE.md",
    "license.md",
    "CODE_OF_CONDUCT.md",
    "code_of_conduct.md",
    "NEWS.md"
  ]
}
